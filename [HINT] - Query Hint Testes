SET SHOWPLAN_ALL ON
OPTION(recompile,querytraceon 9481,USE HINT('FORCE_LEGACY_CARDINALITY_ESTIMATION'))--2014+
OPTION(recompile,querytraceon 2312)--2012-
OPTION (MIN_GRANT_PERCENT = 0.5, USE HINT('ENABLE_PARALLEL_PLAN_PREFERENCE'), USE HINT ('DISABLE_OPTIMIZER_ROWGOAL'))
OPTION (FORCE ORDER);
OPTION (OPTIMIZE FOR UNKNOWN) 
OPTION (LOOP JOIN)
OPTION (RECOMPILE, USE HINT (ENABLE_QUERY_OPTIMIZER_HOTFIXES'))
OPTION (QueryRuleOff BuildSpool)
