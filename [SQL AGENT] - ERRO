USE [master]
GO

CREATE LOGIN [NT SERVICE\SQLAgent$USER] FROM WINDOWS WITH DEFAULT_DATABASE=[master], DEFAULT_LANGUAGE=[us_english]
GO

ALTER SERVER ROLE [sysadmin] ADD MEMBER [NT SERVICE\SQLAgent$USER]
GO



sp_configure 'show advanced options', 1;
RECONFIGURE;
go
sp_configure 'Agent XPs', 1;
RECONFIGURE;


USE [master]
GO

/****** Object:  Login [NT SERVICE\SQLAgent$PROTHEUS]    Script Date: 15/03/2024 14:15:23 ******/
CREATE LOGIN [NT SERVICE\SQLAgent$PROTHEUS] FROM WINDOWS WITH DEFAULT_DATABASE=[master], DEFAULT_LANGUAGE=[us_english]
GO

ALTER SERVER ROLE [sysadmin] ADD MEMBER [NT SERVICE\SQLAgent$PROTHEUS]
GO


-- SERVIÇO DO AGENT INICIA E PARA / SERVIÇO AGENT / AGENT INICIA E PARA / AGENT START E PARA
/****** Object:  Login [NT AUTHORITY\SYSTEM]    Script Date: 08/08/2023 15:13:56 ******/
CREATE LOGIN [NT AUTHORITY\SYSTEM] FROM WINDOWS WITH DEFAULT_DATABASE=[master], DEFAULT_LANGUAGE=[us_english]
GO


CREATE LOGIN [NT SERVICE\MSSQL$PVI] FROM WINDOWS WITH DEFAULT_DATABASE=[master], DEFAULT_LANGUAGE=[us_english]
GO

ALTER SERVER ROLE [sysadmin] ADD MEMBER [NT SERVICE\MSSQL$PVI]
GO

CREATE LOGIN [NT SERVICE\SQLWriter] FROM WINDOWS WITH DEFAULT_DATABASE=[master], DEFAULT_LANGUAGE=[us_english]
GO

ALTER SERVER ROLE [sysadmin] ADD MEMBER [NT SERVICE\SQLWriter]
GO
